#include <iostream>
#include <cstdlib>
#include <cstdio>
#include <ctime>
using namespace std;
int main()
{
    int *ptr1 = new int;
    int *ptr2 = new int;
    *ptr1 = 0 + rand()%16;
    *ptr2 = 0 + rand()%16;
    cout<< "*ptr1: "<< *ptr1<<endl;
    cout<< "*ptr2: "<< *ptr2;
    delete ptr1;
    delete ptr2;
    return 0;
}
